{
  "name": "khaki-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve-ui-only": "ng serve",
    "serve-ui-with-mock": "concurrently -k -s all \"yarn:local-wiremock\" \"yarn:serve-ui-only\"",
    "local-wiremock": "wiremock --global-response-templating --enable-stub-cors",
    "serve-local-api": "ng serve -c localApi",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test-once": "ng test --watch=false",
    "watch-all-modules": "concurrently -k -s all \"yarn:build-*-with-watch\"",
    "build-statistics-with-watch": "ng build khaki-statistics --watch",
    "build-statistics-once": "ng build khaki-statistics",
    "build-profile-with-watch": "ng build khaki-profile --watch",
    "build-profile-once": "ng build khaki-profile",
    "build-settings-with-watch": "ng build khaki-settings --watch",
    "build-settings-once": "ng build khaki-settings",
    "build-info-with-watch": "ng build khaki-info --watch",
    "build-info-once": "ng build khaki-info",
    "build-admin-with-watch": "ng build khaki-admin --watch",
    "build-admin-once": "ng build khaki-admin",
    "build-modules": "concurrently -k -s all -m 1 \"yarn:build-*-once\"",
    "heroku-postbuild": "yarn run build-modules && ng build -c ${KHAKI_ENV_CONFIG}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.6",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "~10.1.6",
    "@angular/compiler": "~10.1.6",
    "@angular/core": "~10.1.6",
    "@angular/forms": "~10.1.6",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/router": "~10.1.6",
    "@auth0/auth0-angular": "^1.2.0",
    "@fortawesome/angular-fontawesome": "0.7.x",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@natr/historian": "^0.2.1",
    "@ngrx/effects": "10.1.2",
    "@ngrx/schematics": "10.1.2",
    "@ngrx/store": "10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@swimlane/ngx-charts": "^16.0.0",
    "d3": "^6.2.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "morphism": "^1.12.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "~10.1.7",
    "@angular/compiler-cli": "~10.1.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash": "^4.17.20",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wiremock": "^2.27.2"
  }
}
